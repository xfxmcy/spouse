function doResize(img,realWidth,realHeight){var w,h;if(realWidth/realHeight>maxWidth/maxHeight){if(realWidth>maxWidth){w=maxWidth;h=realHeight/(realWidth/maxWidth);}else{w=realWidth;h=realHeight;}}else{if(realHeight>maxHeight){h=realHeight;w=realWidth/(realHeight/maxHeight);}else{h=realHeight;w=realWidth;}}
