var elem=$(".head").attr("id","head"),top=elem.offset().top,elemh=elem.height(),iType=$("input[name='toppub']").val(),html=null;var fakehead=$(document.createElement("div")).addClass("addhead"),ie6top=false;fakehead.insertBefore(elem);$(window).scroll(function(){try{var scrolls=$(this).scrollTop(),bodyh=document.body.offsetHeight;fakehead.css({"height":"80px"}).show();if(scrolls>top){if(window.XMLHttpRequest){elem.css({position:"fixed",top:0});}else{if(!ie6top){elem.ie6winFloat("top");elem.css({position:"absolute"});ie6top=true;}}
