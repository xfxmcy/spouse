function bindMobile(fuName,options){var btn_cancel=options&&options.btn_cancel;var win=ZDK.module.window({title:"",content:html1.show(),width:485,mask:true,cache:false,zIndex:9000,allowClose:true,hasIframe:true,ok:"",cancel:btn_cancel||""});if(options){options.alert&&html1.find(".alert-img").html('<em></em>'+options.alert);options.title&&win.resetTitle(options.title);}
