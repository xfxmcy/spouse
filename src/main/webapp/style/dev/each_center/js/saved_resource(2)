function goTop(){$("body").append("<a href=\"javascript:void(0)\" id=\"goTop\"></a>");var obj=document.getElementById("goTop"),$obj=$("#goTop");try{$obj.ie6winFloat(200);}catch(e){};function getScrollTop(){return $(window).scrollTop();}
function setScrollTop(value){$(window).scrollTop(value);}
window.onscroll=function(){if(getScrollTop()>0){var con_width=$(".main-con").width();var body_width=$("body").width();var _right=(body_width-con_width)/2-70;obj.style.display="block";obj.style.right=_right+"px";}else{obj.style.display="none";}};obj.onclick=function(){var goTop=setInterval(scrollMove,5);function scrollMove(){setScrollTop(getScrollTop()/1.1);if(getScrollTop()<1)clearInterval(goTop);}}};if(!$(document.body).hasClass('t5s')){goTop();}
