window["mobileVerify"]=mobileVerify;function changeTips(win){var text=$("#cert-phone").find("input[type='text']").val();$.ajax({url:sendUrl,data:"mobile="+text+"&tips=1",dataType:'jsonp',jsonp:"jsonpcallback",success:function(json){if(json.state==1){html2.find(".phone-number").html(text);html2.find("input[name='vid']").val(json.vid);win.resetOkButton("");win.resetBody(html2);html2.find(".controls").inputEmpty("");function timer(target){target.attr('disabled','disabled');var num=60;target.html('<i></i>(60)').removeClass("butn-green");var time=setInterval(function(){if(num-->0){target.html('<i></i>('+num+')');}else{clearInterval(time);target.removeAttr('disabled').html('<i></i>').addClass("butn-green");}},1000);}
