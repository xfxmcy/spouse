function isLogItems(target){var result=false;if(location.href.indexOf('/main/user')!=-1||location.href.indexOf('/juhe')!=-1){result=$(target).is('.cont .tit .name');}else if(location.href.indexOf('/p.html')!=-1){result=$(target).is('#contentBox .tit a.name');}else if(location.href.indexOf('/main/task')!=-1||location.href.indexOf('/main/service')!=-1||location.href.indexOf('.com/user/')!=-1||location.href.indexOf('.com/topic/')!=-1||location.href.indexOf('.la/user/')!=-1||location.href.indexOf('.la/topic/')!=-1||location.href.indexOf('http://task.zhubajie.com/')!=-1||location.href.indexOf('http://task.t5.zbj.com/')!=-1||location.href.indexOf('http://task.zhubajie.la/')!=-1){result=$(target).is('.cont .name-tit .tit')||$(target).is('#contentBox .title .tit');}else if(location.href.indexOf('/main/tag')!=-1){result=$(target).is('.cont .tags-tit>a')}else if(location.href.indexOf('/main/all')!=-1){if($(target).parents('.sr-list').length>0){result=$(target).is('.sr-list .tags-tit>a')}else if($(target).parents('.relMasterWarp').length>0){result=$(target).is('.relMasterWarp .name a:first-child')}else if($(target).parents('.right').length>0){result=$(target).is('.right .name-tit .tit')}else if($(target).parents('.user-information').length>0){result=$(target).is('.name-tit .tit')}}
return result}
function getLogItems(t){var items=null;if(location.href.indexOf('/main/user')!=-1||location.href.indexOf('/juhe')!=-1){items=$('.cont .tit .name')}else if(location.href.indexOf('/p.html')!=-1){items=$('#contentBox .tit a.name');}else if(location.href.indexOf('/main/task')!=-1||location.href.indexOf('/main/service')!=-1||location.href.indexOf('.com/user/')!=-1||location.href.indexOf('.com/topic/')!=-1||location.href.indexOf('.la/user/')!=-1||location.href.indexOf('.la/topic/')!=-1||location.href.indexOf('http://task.zhubajie.com/')!=-1||location.href.indexOf('http://task.5.zbj.com/')!=-1||location.href.indexOf('http://task.zhubajie.la/')!=-1){items=$('.cont .name-tit .tit');if(items.length==0){items=$('#contentBox .title .tit');}}else if(location.href.indexOf('/main/tag')!=-1){items=$('.cont .tags-tit>a');}else if(location.href.indexOf('/main/all')!=-1){if($(t).parents('.sr-list').length>0){items=$('.sr-list .tags-tit>a')}else if($(t).parents('.relMasterWarp').length>0){items=$('.relMasterWarp .name a:first-child')}else if($(t).parents('.right').length>0){items=$('.right .name-tit .tit')}else if($(t).parents('.user-information').length>0){items=$(t).parents('.user-information').find('.name-tit .tit')}}
return items;}
