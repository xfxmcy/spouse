return false;});win.on("oncancel",function(){var url,type;if(/u.zhubajie.com/.test(location.href)){url="http://u.zhubajie.com/main/disablepop";type="get";}else{url='http://u.zhubajie.com/main/disablepop?jsonpcallback=?';type="jsonp";}
