return mainUrl+postAddr;}
function getExtraInfo(){var url=location.href;var tagMatch=url.match(/topic\/(\d+)/);if(tagMatch){return'-tagid-'+tagMatch[1];}
if(!/^http:\/\/task\./.test(url)){return'';}
var taskMatch=url.match(/(?:\.la|\.com)\/(\d+)/);if(taskMatch){return'-taskid-'+taskMatch[1];}
return'';}});