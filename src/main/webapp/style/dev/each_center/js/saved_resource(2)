function getQuery(){return encodeURIComponent(jQuery.query.get('kw'));}
function getItemId(target){var href=$(target).attr('href');var id=href.replace(/.*\/(\d+).*/,'$1');return id;}
function getIstop(target){var html=$(target.parentNode).html();if(html.indexOf('<i>置顶</i>')!=-1||html.indexOf('<i class="top">置顶</i>')!=-1){return 1;}
return 0;}
function addAbsid(target){var href=target.href;if(href.indexOf('ab_sid')!=-1){return;}
if(href.indexOf('?')==-1){target.href+='?';}else{target.href+='&';}
target.href+='ab_sid='+($('input[name=ab_sid]').val()||'');}
